{
  "trailingComma": "none",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "overrides": [
    {
      "files": "*.test.ts",
      "options": {
        "semi": true
      }
    }
  ],
  "parser": "typescript",
  "$schema": "http://json.schemastore.org/prettierrc",
  "bracketSpacing": true,
  "jsxBracketSameLine": true,
  "printWidth": 80,
  "useTabs": false
}

